{
  "name": "@http404/fuus",
  "version": "0.0.1",
  "description": "A library of frequently used utilities.",
  "keywords": [
    "utilities",
    "utils"
  ],
  "author": "error-four-o-four",
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "dependencies": {
    "@fuus/array": "^0.0.1",
    "@fuus/easing": "^0.0.1",
    "@fuus/main": "^0.0.2",
    "@fuus/number": "^0.0.1",
    "@fuus/random": "^0.0.1",
    "@fuus/string": "^0.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup index.ts --config \"../../tsup.config.js\""
  }
}
